{
  "name": "latex",
  "word": "letters",
  "rules": {
    "program": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_text_mode"
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_escape_char": {
      "type": "STRING",
      "value": "\\"
    },
    "begin_group": {
      "type": "STRING",
      "value": "{"
    },
    "end_group": {
      "type": "STRING",
      "value": "}"
    },
    "math_shift": {
      "type": "STRING",
      "value": "$"
    },
    "inline_math_shift": {
      "type": "STRING",
      "value": "$"
    },
    "display_math_shift": {
      "type": "STRING",
      "value": "$$"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\#\\$%\\^&_\\{\\}~\\\\][^\\#\\$%\\^\\&_\\{\\}~\\\\]*"
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[^a-zA-Z@]"
    },
    "letters": {
      "type": "PATTERN",
      "value": "[a-zA-Z@]+"
    },
    "_trailing_space": {
      "type": "PATTERN",
      "value": "[\\s\\t\\n]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "%"
          },
          {
            "type": "PATTERN",
            "value": ".*\\n?"
          }
        ]
      }
    },
    "_text_mode": {
      "type": "PREC_LEFT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_control_sequence"
            },
            {
              "type": "SYMBOL",
              "name": "text_group"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            },
            {
              "type": "SYMBOL",
              "name": "environment"
            },
            {
              "type": "SYMBOL",
              "name": "verbatim"
            }
          ]
        }
      }
    },
    "_simple_group": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_control_sequence"
          },
          {
            "type": "SYMBOL",
            "name": "inline_math_shift"
          },
          {
            "type": "SYMBOL",
            "name": "display_math_shift"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "text_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_text_mode"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    },
    "_control_sequence": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "control_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "control_word"
        }
      ]
    },
    "verbatim": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "verb_command"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "star"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "verb_body"
        }
      ]
    },
    "environment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_env"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "env_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "close_env"
        }
      ]
    },
    "env_body": {
      "type": "SYMBOL",
      "name": "_text_mode"
    },
    "open_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_env"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "\\s+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_text_mode"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    },
    "close_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "end_env"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "PATTERN",
            "value": "\\s+"
          }
        },
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_text_mode"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_error"
    },
    {
      "type": "SYMBOL",
      "name": "verb_body"
    },
    {
      "type": "SYMBOL",
      "name": "star"
    },
    {
      "type": "SYMBOL",
      "name": "control_symbol"
    },
    {
      "type": "SYMBOL",
      "name": "control_word"
    },
    {
      "type": "SYMBOL",
      "name": "begin_env"
    },
    {
      "type": "SYMBOL",
      "name": "end_env"
    },
    {
      "type": "SYMBOL",
      "name": "verb_command"
    }
  ],
  "inline": []
}
