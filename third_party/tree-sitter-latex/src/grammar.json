{
  "name": "latex",
  "rules": {
    "program": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_text_mode"
        },
        {
          "type": "BLANK"
        }
      ]
    },
    "_escape_char": {
      "type": "STRING",
      "value": "\\"
    },
    "begin_group": {
      "type": "STRING",
      "value": "{"
    },
    "end_group": {
      "type": "STRING",
      "value": "}"
    },
    "math_shift": {
      "type": "STRING",
      "value": "$"
    },
    "inline_math_shift": {
      "type": "STRING",
      "value": "$"
    },
    "display_math_shift": {
      "type": "STRING",
      "value": "$$"
    },
    "text": {
      "type": "PATTERN",
      "value": "[^\\#\\$%\\^&_\\{\\}~\\\\][^\\#\\$%\\^\\&_\\{\\}~\\\\]*"
    },
    "symbol": {
      "type": "PATTERN",
      "value": "[^a-zA-Z@]"
    },
    "letters": {
      "type": "PATTERN",
      "value": "[a-zA-Z@]+"
    },
    "_trailing_space": {
      "type": "PATTERN",
      "value": "[\\s\\t\\n]+"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "%"
          },
          {
            "type": "PATTERN",
            "value": ".*\\n?"
          }
        ]
      }
    },
    "_text_mode": {
      "type": "PREC_RIGHT",
      "value": 0,
      "content": {
        "type": "REPEAT1",
        "content": {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_control_sequence"
            },
            {
              "type": "SYMBOL",
              "name": "text_group"
            },
            {
              "type": "SYMBOL",
              "name": "text"
            },
            {
              "type": "SYMBOL",
              "name": "environment"
            },
            {
              "type": "SYMBOL",
              "name": "verbatim"
            }
          ]
        }
      }
    },
    "_simple_group": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "_control_sequence"
          },
          {
            "type": "SYMBOL",
            "name": "inline_math_shift"
          },
          {
            "type": "SYMBOL",
            "name": "display_math_shift"
          },
          {
            "type": "SYMBOL",
            "name": "text"
          }
        ]
      }
    },
    "text_group": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_group"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_text_mode"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "end_group"
        }
      ]
    },
    "_control_sequence": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "control_symbol"
        },
        {
          "type": "SYMBOL",
          "name": "control_word"
        }
      ]
    },
    "control_symbol": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape_char"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "symbol"
          },
          "named": true,
          "value": "cs"
        }
      ]
    },
    "control_word": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape_char"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "letters"
          },
          "named": true,
          "value": "cs"
        }
      ]
    },
    "verbatim": {
      "type": "SEQ",
      "members": [
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_escape_char"
            },
            {
              "type": "ALIAS",
              "content": {
                "type": "STRING",
                "value": "verb"
              },
              "named": true,
              "value": "cs"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "SYMBOL",
                  "name": "star"
                },
                {
                  "type": "BLANK"
                }
              ]
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "verb"
        }
      ]
    },
    "environment": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "open_env"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "env_body"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "SYMBOL",
          "name": "close_env"
        }
      ]
    },
    "env_body": {
      "type": "SYMBOL",
      "name": "_text_mode"
    },
    "open_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "begin_command"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "begin_group"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "ALIAS",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_simple_group"
                      },
                      "named": true,
                      "value": "env_name"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "end_group"
            }
          ]
        }
      ]
    },
    "begin_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape_char"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "begin"
          },
          "named": true,
          "value": "cs"
        }
      ]
    },
    "close_env": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "end_command"
        },
        {
          "type": "SEQ",
          "members": [
            {
              "type": "SYMBOL",
              "name": "begin_group"
            },
            {
              "type": "CHOICE",
              "members": [
                {
                  "type": "CHOICE",
                  "members": [
                    {
                      "type": "ALIAS",
                      "content": {
                        "type": "SYMBOL",
                        "name": "_simple_group"
                      },
                      "named": true,
                      "value": "env_name"
                    },
                    {
                      "type": "BLANK"
                    }
                  ]
                },
                {
                  "type": "BLANK"
                }
              ]
            },
            {
              "type": "SYMBOL",
              "name": "end_group"
            }
          ]
        }
      ]
    },
    "end_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_escape_char"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "STRING",
            "value": "end"
          },
          "named": true,
          "value": "cs"
        }
      ]
    }
  },
  "extras": [
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "externals": [
    {
      "type": "SYMBOL",
      "name": "_error"
    },
    {
      "type": "SYMBOL",
      "name": "verb"
    },
    {
      "type": "SYMBOL",
      "name": "star"
    },
    {
      "type": "SYMBOL",
      "name": "s_whitespace"
    },
    {
      "type": "SYMBOL",
      "name": "m_whitespace"
    }
  ],
  "inline": []
}
